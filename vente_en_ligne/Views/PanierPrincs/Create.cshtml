@model vente_en_ligne.Models.PanierPrinc

@{
    ViewData["Title"] = "Create";
}

<link type="text/css" rel="stylesheet" href="/css/CreateU.css" />
<div class="login-box">
    <h2>Become Client</h2>
    <div class="user-box">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="input-group">
                <input asp-for="IDU" class="form-control" readonly value="@ViewData["IDU"]" />
            
            </div>
            <div class="input-group">
                <input asp-for="DateCréation" class="form-control" />
                <span asp-validation-for="DateCréation" class="text-danger"></span>
            </div>
            <div class="input-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
